<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_program_oda_tree" model="ir.ui.view">
        <field name="name">view_program_oda_tree</field>
        <field name="model">g2p.program</field>
        <field name="inherit_id" ref="g2p_programs.view_program_list_tree" />
        <field name="arch" type="xml">
            <xpath expr="//tree" position="inside">
                <button
                    type="object"
                    icon="fa-users"
                    class="oe_stat_button"
                    name="open_eligible_beneficiaries_form"
                    string="Beneficiaries list"
                />
            </xpath>
        </field>
    </record>


</odoo>
