<odoo>
    <record id="create_claims_program_wizard_form_view" model="ir.ui.view">
        <field name="name">create_claims_program_wizard_form_view</field>
        <field name="model">g2p.program.create.wizard</field>
        <field name="inherit_id" ref="g2p_programs.create_program_wizard_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']/.." position="inside">
                <field name="is_claims_program" />
            </xpath>
        </field>
    </record>

</odoo>
