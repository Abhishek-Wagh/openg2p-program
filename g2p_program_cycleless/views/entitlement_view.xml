<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--#Tree view -->
    <record id="view_entitlement_inherited_entitlement_tree" model="ir.ui.view">
        <field name="name">view_entitlement_inherited_entitlement_tree</field>
        <field name="model">g2p.entitlement</field>
        <field name="inherit_id" ref="g2p_programs.view_entitlement_tree" />
        <field name="arch" type="xml">
            <field name="cycle_id" position="attributes">
                <attribute name="invisible">True</attribute>
            </field>
        </field>
    </record>

    <!-- Form view -->
    <record id="view_entitlement_inherited_reimbursement_form" model="ir.ui.view">
        <field name="name">view_entitlement_inherit_reimbursement_form</field>
        <field name="model">g2p.entitlement</field>
        <field name="inherit_id" ref="g2p_programs.view_entitlement_form" />
        <field name="arch" type="xml">
            <field name="cycle_id" position="attributes">
                <attribute name="invisible">True</attribute>
            </field>
        </field>
    </record>
</odoo>
